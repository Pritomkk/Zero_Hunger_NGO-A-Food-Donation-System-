@model IEnumerable<Assignment_Zero_Hunger.EF.CollectRequest>

@{
    ViewBag.Title = "DistributeCollectedFood";
}

<h2>DistributeCollectedFood</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CollectRequest_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FoodDscription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FoodType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Quantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Expired_Time)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RestruentDonorId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CollectRequest_Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FoodDscription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FoodType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Quantity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Expired_Time)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RestruentDonorId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>

            @Html.ActionLink("Donate Food", "FoodDistributionByEmployee", new { id = item.CollectRequest_Id, address = item.Address }, new { @class = "btn btn-primary" })

        </td>
    </tr>
}

</table>



<div>
    @Html.ActionLink("Back", "EmployeeDash", null, new { @class = "btn btn-danger" })
</div>